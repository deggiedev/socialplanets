<h2>Active Users</h2>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= button_to(user.username, user, :method => "get") %>
        <ul><br>
            <li><%= "Native Species: #{user.native_species}" %></li>
        </ul>
    </li>
    <br>
  <% end %>
</ul>

<%= button_to('sign up', new_user_path, :method => "get", class: "btn") %>